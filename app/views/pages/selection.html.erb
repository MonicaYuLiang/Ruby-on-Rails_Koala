		<script src="assets/jquery.min.js"></script>
		<script src="assets/jquery.scrolly.min.js"></script>
		<script src="assets/jquery.dropotron.min.js"></script>
		<script src="assets/jquery.scrollex.min.js"></script>
		<script src="assets/skel.min.js"></script>
		<script src="assets/skel-layers.min.js"></script>
		<script src="assets/init.js"></script>
		<noscript>
			<link rel="stylesheet" href="css/skel.css" />
			<link rel="stylesheet" href="css/style.css" />
			<link rel="stylesheet" href="css/style-xlarge.css" />
		</noscript>
